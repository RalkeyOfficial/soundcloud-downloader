	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Debugger: Download song with NO GO+",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/soundcloud_downloader.py",
			"console": "integratedTerminal",
			"args": [
				"--url",
				"https://soundcloud.com/laugui-anthropoly/run-boy-run-woodkid", // this song is a blast
				"--output",
				"output",
				"--codec",
				"mp3",
			]
		},
		{
			"name": "Python Debugger: Download song with GO+",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/soundcloud_downloader.py",
			"console": "integratedTerminal",
			"args": [
				"--url",
				"https://soundcloud.com/woodkid/run-boy-run",
				"--output",
				"output",
				"--codec",
				"mp3",
			]
		},
		{
			"name": "Python Debugger: Download song with GO+ OGG Opus",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/soundcloud_downloader.py",
			"console": "integratedTerminal",
			"args": [
				"--url",
				"https://soundcloud.com/woodkid/run-boy-run",
				"--output",
				"output",
				"--codec",
				"opus",
			]
		},
		{
			"name": "Python Debugger: Download song with GO+ OGG Vorbis",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/soundcloud_downloader.py",
			"console": "integratedTerminal",
			"args": [
				"--url",
				"https://soundcloud.com/woodkid/run-boy-run",
				"--output",
				"output",
				"--codec",
				"vorbis",
			]
		}
	]
}
